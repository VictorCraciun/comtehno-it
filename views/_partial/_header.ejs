                        </div>
                    </div>
                    <a href="/contact" class="nav-item nav-link">Contact Us</a>
                </div>
                <% if(currentUser != null) { %>
                    <a href="/post" class="nav-item nav-link">My posts</a>
                    <a href="#" class="btn btn-sm btn-light rounded-pill py-2 px-4 d-none d-lg-block">Welcome <%= currentUser.username %></a>
                    <a href="/logout" class="btn btn-sm btn-light rounded-pill py-2 px-4 d-none d-lg-block">Logout</a>
                <% } %>
                <% if(currentUser == null) { %>
                    <a href="/login" class="btn btn-sm btn-light rounded-pill py-2 px-4 d-none d-lg-block">Log IN</a>
                    <a href="/signup" class="btn btn-sm btn-light rounded-pill py-2 px-4 d-none d-lg-block">Sign UP</a>
                    <% } %>
            </div>
        </nav>
        <p></p>
        <div class="container">

            <% if(error && error.length > 0) { %>
                <div class="alert alert-danger" role="alert"> 
                    <%= error %>  
                </div>
            <% } %>

            <% if(info && info.length > 0) { %>
                <div class="alert alert-info" role="alert"> 
                    <%= info %>  
                </div>
            <% } %>
        </div>
